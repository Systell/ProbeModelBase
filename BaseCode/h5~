fName=${PWD##*/}
fName+=".js"
if [ -f Export/html5/bin/$fName ];
then
	fileTimeOld=$(stat -c %Y Export/html5/bin/$fName)
	sudo openfl build html5 -Dv2;
	fileTimeNew=$(stat -c %Y Export/html5/bin/$fName)
	if [ $fileTimeOld -lt $fileTimeNew ];
	then
		xdg-open Export/html5/bin/index.html;
	fi
else
	sudo openfl build html5 -Dv2;
	if [ -f Export/html5/bin/$fName ];
	then
		xdg-open Export/html5/bin/index.html;
	fi
fi
